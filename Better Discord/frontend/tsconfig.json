{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "react-jsx",
    "outDir": "./dist",
    "noEmit": false,  // MUST CHANGE THIS
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "expo": ["node_modules/expo"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
